<RapidFit>
<ParameterSet>
<PhysicsParameter>
<Name>sigma</Name>
<Value>2.</Value>
<Minimum>-10.</Minimum>
<Maximum>10.</Maximum>
</PhysicsParameter>
<PhysicsParameter>
<Name>sigma2</Name>
<Value>5.</Value>
<Minimum>-10.</Minimum>
<Maximum>10.</Maximum>
</PhysicsParameter>
<PhysicsParameter>
<Name>f_sig1</Name>
<Value>0.6</Value>
<Minimum>0.</Minimum>
<Maximum>1.</Maximum>
</PhysicsParameter>
</ParameterSet>
<Minimiser>Minuit</Minimiser>
<FitFunction>
<FunctionName>NegativeLogLikelihoodThreaded</FunctionName>
#<UseGSLNumericalIntegration>True</UseGSLNumericalIntegration>
</FitFunction>
<ToFit>
<PDF>
<Name>SimpleDoubleGauss</Name>
<ConfigurationParameter>PlotComponents:True</ConfigurationParameter>
</PDF>
<DataSet>
<Source>Foam</Source>
<NumberEvents>10000</NumberEvents>
<PhaseSpaceBoundary>
<Observable>
<Name>x</Name>
<Minimum>-20.</Minimum>
<Maximum>20.</Maximum>
</Observable>
</PhaseSpaceBoundary>
</DataSet>
</ToFit>
<Output>
	<ComponentProjection>
		<Name>x</Name>
		<DrawPull>True</DrawPull>
	</ComponentProjection>
<Scan>
<Name>sigma</Name>
<Sigma>3</Sigma>
<Points>100</Points>
</Scan>
</Output>
</RapidFit>
